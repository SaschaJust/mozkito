                                <reporter username="asgeirn">Asgeir Storesund Nilsen</reporter>
                        <labels>
                    </labels>
                <created>Sat, 4 Mar 2006 05:24:40 -0600</created>
                <updated>Mon, 9 Nov 2009 17:00:31 -0600</updated>
                    <resolved>Fri, 25 Sep 2009 00:17:56 -0500</resolved>
