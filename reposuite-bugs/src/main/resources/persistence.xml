<?xml version="1.0"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0">
	<persistence-unit name="Reposuite" transaction-type="RESOURCE_LOCAL">
		<provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
		<class>de.unisaarland.cs.st.reposuite.bugs.tracker.model.Comment</class>
		<class>de.unisaarland.cs.st.reposuite.bugs.tracker.model.CommentPrimaryKey</class>
		<class>de.unisaarland.cs.st.reposuite.bugs.tracker.model.History</class>
		<class>de.unisaarland.cs.st.reposuite.bugs.tracker.model.HistoryElement</class>
		<class>de.unisaarland.cs.st.reposuite.bugs.tracker.model.Report</class>
		<properties>
			<property name="openjpa.Log" value="slf4j"/>
			<property name="openjpa.Multithreaded" value="true"/>
			<property name="openjpa.jdbc.SynchronizeMappings" value="validate"/>
			<property name="openjpa.ConnectionURL" value="jdbc:postgres://quentin.cs.uni-saarland.de/miner"/>
			<property name="openjpa.ConnectionDriverName" value="org.postgresql.Driver"/>
			<property name="openjpa.ConnectionPassword" value="miner"/>
			<property name="openjpa.ConnectionUserName" value="miner"/>
		</properties>
	</persistence-unit>
</persistence>
